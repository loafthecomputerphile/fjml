{
    "Controls":[
        {
            "var_name":"sheet",
            "control_type":"SheetTable",
            "settings":{
                "engine":"openpyxl",
                "table_params":null,
                "sheet_params":[
                    {
                        "sheets":"remaining",
                        "sheet_filter":null,
                        "settings":{
                            "column_letters":["A", "B", "C"],
                            "title_row":1,
                            "start_row":2,
                            "end_row":140,
                            "editable_col_letters":["A", "B", "C"]
                        }
                    }
                ],
                "expand":11,
                "filename":"./tests/test_files/test_data.xlsx"
            }
        },
        {
            "var_name":"submit_sheet_selection_btn",
            "control_type":"Container",
            "settings":{
                "content":{
                    "control_type":"ElevatedButton",
                    "settings":{
                        "text":"Submit",
                        "style":{"ref":"submit_btn_style"},
                        "on_click":{"func":"set_sheet_data"},
                        "expand":true,
                        "expand_loose":true,
                        "width":500
                    }
                },
                "expand":true,
                "height":50,
                "padding":{
                    "control_type":"padding_symmetric",
                    "settings":{
                        "horizontal":20
                    }
                }
            }
        },
        {
            "var_name":"excel_sheet_view",
            "control_type":"Card",
            "settings":{
                "content":{
                    "control_type":"Container",
                    "settings":{
                        "content":{
                            "control_type":"Column",
                            "settings":{
                                "controls":[
                                    {"ref":"sheet"},
                                    {"ref":"submit_sheet_selection_btn"}
                                ],
                                "horizontal_alignment":"center",
                                "alignment":"center"
                            }
                        },
                        "expand":true,
                        "padding":{"ref":"padding_constant_2"}
                    }
                },
                "expand":1,
                "color":"grey100",
                "surface_tint_color":"grey100"
            }
        }
    ]
}