{
    "Controls":[
        {
            "var_name":"preprocessor_selector",
            "control_type":"Container",
            "settings":{
                "content":{
                    "control_type":"Row",
                    "settings":{
                        "controls":{
                            "control_type":"loop",
                            "depth":1,
                            "iterator":{"ref":"preprocessors"},
                            "control":{
                                "control_type":"FilledButton",
                                "settings":{
                                    "data":{"control_type":"loop_index", "idx":[0,0]},
                                    "text":{"control_type":"loop_index", "idx":[0,1]},
                                    "style":{"ref":"processor_btn_style"},
                                    "height":40,
                                    "width":200,
                                    "on_click":{"func":"processor_selected"}
                                }
                            }
                        },
                        "scroll":"always",
                        "vertical_alignment":"center",
                        "alignment":"center"
                    }
                },
                "unpack":{"ref":"container_ui_constants_1"}
            }
        },
        {
            "var_name":"preprocessor_kwargs",
            "control_type":"Container",
            "settings":{
                "content":{
                    "control_type":"Column",
                    "settings":{
                        "controls":[],
                        "scroll":"always",
                        "spacing":6,
                        "horizontal_alignment":"start",
                        "alignment":"center"
                    }
                },
                "expand":true,
                "padding":{"ref":"padding_constant_1"},
                "margin":{
                    "control_type":"margin_symmetric",
                    "settings":{
                        "horizontal":20,
                        "vertical":2
                    }
                },
                "border_radius":10,
                "bgcolor":"bluegrey50",
                "alignment":{"control_type": "alignment.top_center"}
            }
        },
        {
            "var_name":"excel_data_container",
            "control_type":"Card",
            "settings":{
                "content":{
                    "control_type":"Container",
                    "settings":{
                        "content":{
                            "control_type":"Column",
                            "settings":{
                                "controls":[
                                    {"ref":"preprocessor_selector"},
                                    {"ref":"preprocessor_kwargs"}
                                ],
                                "horizontal_alignment":"center",
                                "alignment":"center"
                            }
                        },
                        "expand":true,
                        "padding":{"ref":"padding_constant_2"}
                    }
                },
                "col":{"xs":12, "sm":12,"xl":6},
                "surface_tint_color":"grey100",
                "color":"grey100"
            }
        }
    ]
}