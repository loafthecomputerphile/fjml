{
    "Controls":[
        {
            "var_name":"route_success",
            "control_type":"Text",
            "settings":{
                "value":"Route Test Passed",
                "size":36,
                "weight":"w700"
            }
        },
        {
            "var_name":"back_btn",
            "control_type":"Row",
            "settings":{
                "controls":[
                    {
                        "control_type":"RouteBtn",
                        "settings":{
                            "text":"Return",
                            "route_path":"/"
                        }
                    }
                ],
                "expand":true,
                "unpack":{"styling":"row_center"}
            }
        },
        {
            "var_name":"route_test_btn",
            "control_type":"RouteBtn",
            "settings":{
                "text":"Test Route",
                "route_path":"/route_test"
            }
        },
        {
            "var_name":"route_test_success_display",
            "control_type":"Container",
            "settings":{
                "content":{
                    "control_type":"Column",
                    "settings":{
                        "controls":[
                            {"refs":"route_success"},
                            {
                                "control_type":"Container",
                                "settings":{
                                    "content":{"refs":"back_btn"},
                                    "alignment":{"control_type":"alignment.center"}
                                }
                            }
                        ],
                        "unpack":{"styling":"Route.display_col"}
                    }
                },
                "unpack":{"styling":"Route.display_container"}
            }
        }
    ]
}