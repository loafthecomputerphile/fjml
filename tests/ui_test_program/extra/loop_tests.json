{
    "Controls":[
        {
            "var_name":"refserence_test_control",
            "control_type":"Chip",
            "settings":{
                "label":{
                    "control_type":"Text",
                    "settings":{
                        "value":"test passed"
                    }
                },
                "bgcolor":"blue200",
                "disabled_color":"blue100",
                "autofocus":true,
                "on_select":{"eval":"lambda e: self.page.update()"}
            }
        },
        {
            "var_name":"callable_loop_test_content",
            "control_type":"Row",
            "settings":{
                "controls":{
                    "control_type":"loop",
                    "depth":1,
                    "iterator":{"code_refs":"loop_test_content"},
                    "control":{
                        "call":"callable_loop_test_func",
                        "settings":{
                            "name":{
                                "control_type":"loop_index", 
                                "idx":[0, 0]
                            }
                        }
                    }
                },
                "scroll":"always",
                "_unpack":{"styling":"vert_row_center"}
            }
        },
        {
            "var_name":"callable_loop_test",
            "control_type":"Container",
            "settings":{
                "content":{
                    "control_type":"Column",
                    "settings":{
                        "controls":[
                            {
                                "control_type":"Text",
                                "settings":{
                                    "value":"Callable Loop Test",
                                    "size":20,
                                    "weight":"w700",
                                    "color":"black"
                                }
                            },
                            {"refs":"callable_loop_test_content"}
                        ],
                        "_unpack":{"styling":"col_center"}
                    }
                },
                "_unpack":{"styling":"Loop.loop_test"}
            }
        },
        {
            "var_name":"control_loop_test_content",
            "control_type":"Row",
            "settings":{
                "controls":{
                    "control_type":"loop",
                    "depth":1,
                    "iterator":{"code_refs":"loop_test_content"},
                    "control":{
                        "control_type":"Chip",
                        "settings":{
                            "label":{
                                "control_type":"Text",
                                "settings":{
                                    "value":{
                                        "control_type":"loop_index", 
                                        "idx":[0, 0]
                                    }
                                }
                            },
                            "bgcolor":{
                                "control_type":"colors",
                                "attr":"GREEN_200"
                            },
                            "disabled_color":"green100",
                            "autofocus":true,
                            "on_select":{"eval":"lambda e: self.update()"}
                        }
                    }
                },
                "scroll":"always",
                "_unpack":{"styling":"vert_row_center"}
            }
        },
        {
            "var_name":"control_loop_test",
            "control_type":"Container",
            "settings":{
                "content":{
                    "control_type":"Column",
                    "settings":{
                        "controls":[
                            {
                                "control_type":"Text",
                                "settings":{
                                    "value":"Control Loop Test (and eval test)",
                                    "size":20,
                                    "weight":"w700",
                                    "color":"black"
                                }
                            },
                            {"refs":"control_loop_test_content"}
                        ],
                        "_unpack":{"styling":"col_center"}
                    }
                },
                "_unpack":{"styling":"Loop.loop_test"}
            }
        },
        {
            "var_name":"refserence_loop_test_content",
            "control_type":"Row",
            "settings":{
                "controls":{
                    "control_type":"loop",
                    "depth":1,
                    "iterator":{"code_refs":"loop_test_content"},
                    "control":{"refs":"refserence_test_control"}
                },
                "scroll":"always",
                "_unpack":{"styling":"vert_row_center"}
            }
        },
        {
            "var_name":"refserence_loop_test",
            "control_type":"Container",
            "settings":{
                "content":{
                    "control_type":"Column",
                    "settings":{
                        "controls":[
                            {
                                "control_type":"Text",
                                "settings":{
                                    "value":"refserence Loop Test (and eval test)",
                                    "size":20,
                                    "weight":"w700",
                                    "color":"black"
                                }
                            },
                            {"refs":"refserence_loop_test_content"}
                        ],
                        "_unpack":{"styling":"col_center"}
                    }
                },
                "_unpack":{"styling":"Loop.loop_test"}
            }
        },
        {
            "var_name":"loop_test_controls",
            "control_type":"Column",
            "settings":{
                "controls":[
                    {"refs":"callable_loop_test"},
                    {"refs":"control_loop_test"},
                    {"refs":"refserence_loop_test"}
                ],
                "expand":true
            }
        },
        {
            "var_name":"loop_test_display",
            "control_type":"Container",
            "settings":{
                "content":{
                    "control_type":"Column",
                    "settings":{
                        "controls":[
                            {
                                "control_type":"Text",
                                "settings":{
                                    "value":"Loop Tests",
                                    "size":{
                                        "code_refs":"txt_size",
                                        "idx":0
                                    },
                                    "weight":"w800"
                                }
                            },
                            {"refs":"loop_test_controls"}
                        ],
                        "spacing":8,
                        "expand":true
                    }
                },
                "_unpack":{"styling":"Loop.display"}
            }
        }
    ]
}